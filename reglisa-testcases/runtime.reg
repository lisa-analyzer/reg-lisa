n := 0;
even := 0;
odd := 1;

(
    n < 10 ? ;
    (
        y := n;

        (
            !(y <= 1)? ;
                y := y - 2
        )* ;

        (
            (y = 0)? ;
                even := even + n
        );
        (
            (y = 1) ? ;
                odd := odd * n
        );

        n := n + 1
    )
)*